command=/usr/share/i3blocks/$BLOCK_NAME
separator_block_width=15
markup=none

[spotify]
command=~/.config/i3blocks/spotify.sh
interval=5
signal=10

[volume]
label=VOL
instance=Master
interval=1
signal=10

[iface]
